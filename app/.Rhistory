lapply(htmltools::HTML)
# This if controls for no circles showing if rates are selected
if (data_type_input() %in% list("COVID_CASE_COUNT", "COVID_DEATH_COUNT", "TOTAL_COVID_TESTS")) {
map <- leaflet(char_zips) %>%
